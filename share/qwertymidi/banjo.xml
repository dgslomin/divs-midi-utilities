<?xml version="1.0"?>

<!--
Five string banjo.
-->

<qwertymidi>

<map key="escape" action="exit" />
<map key="f1" action="stay-down-octave" />
<map key="f2" action="stay-up-octave" />
<map key="f3" action="stay-down-note" />
<map key="f4" action="stay-up-note" />
<map key="shift" action="panic" /><!-- moved here so we can use space for the drone -->

<map key="1" action="D4" />
<map key="2" action="D#4" />
<map key="3" action="E4" />
<map key="4" action="F4" />
<map key="5" action="F#4" />
<map key="6" action="G4" />
<map key="7" action="G#4" />
<map key="8" action="A4" />
<map key="9" action="A#4" />
<map key="0" action="B4" />
<map key="minus" action="C5" />
<map key="equals" action="C#5" />
<map key="backspace" action="D5" />

<map key="Q" action="B3" />
<map key="W" action="C4" />
<map key="E" action="C#4" />
<map key="R" action="D4" />
<map key="T" action="D#4" />
<map key="Y" action="E4" />
<map key="U" action="F4" />
<map key="I" action="F#4" />
<map key="O" action="G4" />
<map key="P" action="G#4" />
<map key="open-bracket" action="A4" />
<map key="close-bracket" action="A#4" />
<map key="backslash" action="B4" />

<map key="A" action="G3" />
<map key="S" action="G#3" />
<map key="D" action="A3" />
<map key="F" action="A#3" />
<map key="G" action="B3" />
<map key="H" action="C4" />
<map key="J" action="C#4" />
<map key="K" action="D4" />
<map key="L" action="D#4" />
<map key="semicolon" action="E4" />
<map key="single-quote" action="F4" />
<map key="enter" action="F#4" />

<map key="Z" action="D3" />
<map key="X" action="D#3" />
<map key="C" action="E3" />
<map key="V" action="F3" />
<map key="B" action="F#3" />
<map key="N" action="G3" />
<map key="M" action="G#3" />
<map key="comma" action="A3" />
<map key="period" action="A#3" />
<map key="slash" action="B3" />

<map key="space" action="G4" /><!-- drone string -->

</qwertymidi>
