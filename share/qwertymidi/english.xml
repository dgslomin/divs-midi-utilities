<?xml version="1.0"?>

<!--
English concertina.

Point your hands inwards.
-->

<qwertymidi>

<map key="escape" action="exit" />
<map key="f1" action="stay-down-octave" />
<map key="f2" action="stay-up-octave" />
<map key="f3" action="stay-down-note" />
<map key="f4" action="stay-up-note" />
<map key="space" action="shift-up-octave" />

<!-- Left hand -->

<map key="Q" action="A2" />
<map key="1" action="Ab2" />

<map key="A" action="C3" />
<map key="Z" action="C#3" />

<map key="W" action="E3" />
<map key="2" action="Eb3" />

<map key="S" action="G3" />
<map key="X" action="G#3" />

<map key="E" action="B3" />
<map key="3" action="Bb3" />

<map key="D" action="D4" />
<map key="C" action="D#4" />

<map key="R" action="F4" />
<map key="4" action="F#4" />

<map key="F" action="A4" />
<map key="V" action="Ab4" />

<map key="T" action="C5" />
<map key="5" action="C#5" />

<map key="G" action="E5" />
<map key="B" action="Eb5" />

<map key="Y" action="G5" />
<map key="6" action="G#5" />

<map key="H" action="B5" />

<!-- Right hand -->

<map key="close-bracket" action="B2" />
<map key="backspace" action="Bb2" />

<map key="single-quote" action="D3" />
<map key="slash" action="D#3" />

<map key="open-bracket" action="F3" />
<map key="equals" action="F#3" />

<map key="semicolon" action="A3" />
<map key="period" action="Ab3" />

<map key="P" action="C4" />
<map key="minus" action="C#4" />

<map key="L" action="E4" />
<map key="comma" action="Eb4" />

<map key="O" action="G4" />
<map key="0" action="G#4" />

<map key="K" action="B4" />
<map key="M" action="Bb4" />

<map key="I" action="D5" />
<map key="9" action="D#5" />

<map key="J" action="F5" />
<map key="N" action="F#5" />

<map key="U" action="A5" />
<map key="8" action="Ab5" />

</qwertymidi>
