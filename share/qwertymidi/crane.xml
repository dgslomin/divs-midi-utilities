<?xml version="1.0"?>

<!--
Crane/Triumph/Butterworth duet concertina layout.
-->

<qwertymidi>

<map key="escape" action="exit" />
<map key="f1" action="stay-down-octave" />
<map key="f2" action="stay-up-octave" />
<map key="f3" action="stay-down-note" />
<map key="f4" action="stay-up-note" />
<map key="space" action="shift-up-octave" />

<!-- Left hand -->

<map key="1" action="Eb4" />
<map key="2" action="E4" />
<map key="3" action="G4" />
<map key="4" action="F4" />
<map key="5" action="F#4" />

<map key="Q" action="Bb3" />
<map key="W" action="B3" />
<map key="E" action="D4" />
<map key="R" action="C4" />
<map key="T" action="C#4" />

<map key="A" action="F#3" />
<map key="S" action="F3" />
<map key="D" action="A3" />
<map key="F" action="G3" />
<map key="G" action="G#3" />

<map key="Z" action="C#3" />
<map key="X" action="C3" />
<map key="C" action="E3" />
<map key="V" action="D3" />
<map key="B" action="D#3" />

<!-- Right hand -->

<map key="6" action="Eb5" />
<map key="7" action="E5" />
<map key="8" action="G5" />
<map key="9" action="F5" />
<map key="0" action="F#5" />

<map key="Y" action="Bb4" />
<map key="U" action="B4" />
<map key="I" action="D5" />
<map key="O" action="C5" />
<map key="P" action="C#5" />

<map key="H" action="F#4" />
<map key="J" action="F4" />
<map key="K" action="A4" />
<map key="L" action="G4" />
<map key="semicolon" action="G#4" />

<map key="N" action="C#4" />
<map key="M" action="C4" />
<map key="comma" action="E4" />
<map key="period" action="D4" />
<map key="slash" action="D#4" />

</qwertymidi>
