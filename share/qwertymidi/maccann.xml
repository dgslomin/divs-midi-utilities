<?xml version="1.0"?>

<!--
Maccann duet concertina 47 key layout approximated in straight rows.
-->

<qwertymidi>

<map key="escape" action="exit" />
<map key="f1" action="stay-down-octave" />
<map key="f2" action="stay-up-octave" />
<map key="f3" action="stay-down-note" />
<map key="f4" action="stay-up-note" />
<map key="shift" action="shift-up-octave" />
<map key="control" action="shift-down-octave" />
<map key="space" action="panic" />

<!-- Left hand -->

<map key="backtick" action="G#4" />
<map key="1" action="G4" />
<map key="2" action="B4" />
<map key="3" action="A4" />
<map key="4" action="C5" />

<map key="tab" action="C#4" />
<map key="Q" action="C4" />
<map key="W" action="E4" />
<map key="E" action="D4" />
<map key="R" action="F4" />
<map key="T" action="F#4" />

<!-- <map key="caps-lock" action="G#3" /> disabled due to awkward caps lock behavior -->
<map key="A" action="G3" />
<map key="S" action="A3" />
<map key="D" action="Eb4" />
<map key="F" action="B3" />
<map key="G" action="Bb3" />

<map key="Z" action="C3" />
<map key="X" action="E3" />
<map key="V" action="F3" />
<map key="B" action="F#3" />

<!-- Right hand -->

<map key="f5" action="G6" /><!-- whatever's northwest of the 7 key -->
<map key="f6" action="G6" />
<map key="f7" action="G6" />
<map key="f8" action="G6" />

<map key="7" action="D6" />
<map key="8" action="E6" />
<map key="9" action="Bb5" />
<map key="0" action="F6" />
<map key="minus" action="F#6" />

<map key="Y" action="G#5" />
<map key="U" action="G5" />
<map key="I" action="B5" />
<map key="O" action="A5" />
<map key="P" action="C6" />
<map key="open-bracket" action="C#6" />

<map key="H" action="C#5" />
<map key="J" action="C5" />
<map key="K" action="E5" />
<map key="L" action="D5" />
<map key="semicolon" action="F5" />
<map key="single-quote" action="F#5" />

<map key="N" action="G#4" />
<map key="M" action="G4" />
<map key="comma" action="A4" />
<map key="period" action="Eb5" />
<map key="slash" action="B4" />

</qwertymidi>
